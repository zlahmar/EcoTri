{
  "expo": {
    "name": "EcoTri",
    "slug": "ecotri",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Cette app utilise la caméra pour scanner les déchets",
        "NSPhotoLibraryUsageDescription": "Cette app accède aux photos pour analyser les déchets"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.anonymous.ecotri"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "5ab86759-a642-4d6d-8a14-65c2cdb506d9"
      }
    }
  }
}
